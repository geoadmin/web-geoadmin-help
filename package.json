{
    "name": "web-geoadmin-help",
    "version": "1.0.0",
    "description": "Help for geoadmin",
    "private": true,
    "scripts": {
        "clean": "rm -rf dist/*",
        "build": "npm run generate:data && vite build",
        "build:dev": "npm run build -- --mode development",
        "start:dev": "vite --port 8080 --host --cors",
